<div class="main-content">
	<div class="card">
		<div class="card-header">
			<h4 class="card-title">Detalhes</h4>
		</div>

		<div class="card-body form-type-round">
						<address>
						<strong>Competicao</strong><br>
							<%= @competicaos_evento.competicao.name rescue nil %>
						</address>
						<address>
						<strong>Evento</strong><br>
							<%= @competicaos_evento.evento.name rescue nil %>
						</address>
							
		<%= link_to 'Editar', edit_competicaos_evento_path(@competicaos_evento), class: 'btn btn-bold btn-info' %>
		<%= link_to 'Voltar', competicaos_eventos_path, class: 'btn btn-bold btn-default' %>
		</div>
	</div>
</div>